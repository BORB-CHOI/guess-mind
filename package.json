{
  "name": "guess-mind",
  "version": "1.0.0",
  "description": "Real Time Drawing Game buit with SocketIO, Gulp and Node",
  "main": "index.js",
  "repository": "https://github.com/BORB-CHOI/guess-mine.git",
  "author": "Your Name <you@example.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babelify": "^10.0.0",
    "del": "^6.0.0",
    "eslint": "^7.11.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-browserify": "^0.5.1",
    "gulp-csso": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.5",
    "pug": "^3.0.0",
    "socket.io": "^2.3.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server --ignore assets/ --ignore src/static",
    "dev:assets": "gulp",
    "build:server": " babel src --out-dir dist --ignore src/assets --ignore src/static ",
    "build:assets": "gulp build",
    "copyAll": "xcopy \"src/static\" /S \"dist/static\" && xcopy \"src/views\" \"dist/views\" /S",
    "prebuild": "rd /s \"dist\"",
    "build": "yarn build:server && yarn build:assets && yarn copyAll",
    "start": "node build/server.js"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1"
  }
}
